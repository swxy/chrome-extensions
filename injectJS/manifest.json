{
	"manifest_version": 2,
	"name": "页面脚本注入",
	"version": "1.0.0",
	"description": "动态添加js脚本，动态注入到匹配的页面的中",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"webNavigation"
	],
	"background" : {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
	]
}